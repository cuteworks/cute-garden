{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "sourceMap": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "removeComments": true,
    "noImplicitAny": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnusedLabels": false,
    "outDir": "static/js/bin"
  },
  "include": [
    "src/ts/"
  ]
}